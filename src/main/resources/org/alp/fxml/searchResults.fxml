<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.alp.components.controllers.SearchResultsController">
    <columnConstraints>
        <ColumnConstraints percentWidth="80"/>
        <ColumnConstraints percentWidth="20" halignment="LEFT"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="100" valignment="TOP"/>
    </rowConstraints>

    <TableView fx:id="resultsTable" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button fx:id="showButton" onMouseClicked="#onShowButtonMouseClick" GridPane.columnIndex="1" GridPane.rowIndex="0" text="Open Graph"/>
</GridPane>
